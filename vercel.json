{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/spa"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/((?!js/|css/|fonts/|icons/|img/|favicon.ico|mode-dbml.js|mode-dbml2.js|test-ai-service.js).*)",
      "destination": "/index.html"
    }
  ]
}