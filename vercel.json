{
  "version": 2,
  "rootDirectory": "web",
  "buildCommand": "npm install && npm run build",
  "outputDirectory": "dist/spa",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/((?!js/|css/|fonts/|icons/|img/|favicon.ico|mode-dbml.js|mode-dbml2.js|test-ai-service.js).*)",
      "destination": "/index.html"
    }
  ]
}